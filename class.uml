@startuml
class Player {
   Position position;
   double velociteX;
   double velociteY;
}

class Position {
   double x
   double y
   + void setX()
   + void setY()
   + double getX()
   + double getY()
}

class Game {
   Player player;
   + void run()
}

interface Object {
   + boolean IsInside(Player p)
}

class Spike {
   Position position
}

class Block {
   Position position
}

class Air {
   Position position
}

Object <|-- Spike
Object <|-- Block
Object <|-- Air

class Map {
   List<Objetc> maps
}

Player --* Position 
Game --* Player
Game --* Map
Map --* "maps" Object
@enduml
